{{#news}}
<div class="new-wrapper">
<h3>{{user}}:</h3>
<h4 data-id={{_id}}>{{text}}</h4> <button class="like">Like it!</button>
<p>{{date}}  <strong>{{#if likes}}Likes:{{likes}}{{else}}Likes: 0{{/if}}</strong></p> 
{{#if commments}}
<p>Comments:<p>
{{#comments}}
<p>{{text}}</p>
{{/comments}}
{{else}}
<p>No comments yet!</p>
{{/if}}
</div>
{{/news}}